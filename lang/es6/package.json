{
  "name": "tango",
  "private": true,
  "version": "2.3.1",
  "description": "Implementation of some algorithms using ECMAScript 6",
  "main": "bundle.js",
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "mocha": "^3.0.2",
    "should": "^11.1.0",
    "webpack": "^1.12.11"
  },
  "scripts": {
    "test": "tar xjkvf test/graph/scc.tar -C test/graph/ --overwrite && webpack && mocha",
    "coverage": "tar xjkvf test/graph/scc.tar -C test/graph/ --overwrite && webpack && istanbul cover _mocha -- -R spec",
    "start": "wget -nc -P test/graph/ https://github.com/scotv/algo-js/releases/download/2.7182818284/scc99.7z && 7z e test/graph/scc99.7z -otest/graph/ -aoa"
  },
  "config": {
    "blanket": {
      "pattern": "//^((?!/node_modules/)(?!/test/).)*$/",
      "data-cover-never": [
        "test.js",
        "bundle.js"
      ],
      "data-cover-flags": {
        "branchTracking": true
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/scozv/tango.git"
  },
  "keywords": [
    "algorithm",
    "es6"
  ],
  "author": "Scott LIU",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/scozv/tango/issues"
  },
  "homepage": "https://scozv.github.com/tango"
}
